<body>

{% for message in get_flashed_messages() %}
<div class="alert alert-warning" role="alert">
  {{ message }}
</div>
{%endfor%}

    {% include "components/navbar.html.jinja" %}




    <form action="/signup" method="post">

    <main class="form-signin w-100 m-auto">
  <form>
    <h1 class="h3 mb-3 fw-normal">Please sign up</h1>

    <div class="form-floating">
      <input type="text" class="form-control" id="floatingInput" placeholder="First name" name = "first_name" required value="{{ request.form.get('first_name', '' ) }}">
      <label for="floatingInput">First Name</label>
    </div>
    <div class="form-floating">
      <input type="text" class="form-control" id="floatingInput" placeholder="Last name" name = "last_name" required value="{{ request.form.get('last_name', '' ) }}">
      <label for="floatingPassword">Last Name</label>
    </div>
    <div class="form-floating">
      <input type="email" class="form-control" id="floatingInput" placeholder="name@example.com" name = "email" required value="{{ request.form.get('email', '' ) }}">
      <label for="floatingPassword">Email Address</label>
    </div>
    <div class="form-floating">
      <input type="password" class="form-control" id="floatingPassword" placeholder="Password" name = "password" required>
      <label for="floatingPassword">Password</label>
    </div>
    <div class="form-floating">
      <input type="password" class="form-control" id="floatingPassword" placeholder="Password" name = "conpassword" required>
      <label for="floatingPassword">Password again</label>
    </div>

    <h4> Make sure the password has:</h4>
    <button class="btn btn-primary w-100 py-2" type="submit">Beep Boop(Create Account!)</button>
  </form>
</main>
<script src="/docs/5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>

